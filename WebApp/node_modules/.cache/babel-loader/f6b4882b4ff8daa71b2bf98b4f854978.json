CAAnB,E;;AAAA,C;;;;;uBAhBF,mBAAA,CAmBM,KAnBN,EAAA,UAAA,EAmBM,CAlBJ,mBAAA,CASM,KATN,EAAA,UAAA,EASM,CARJ,UAQI,EANJ,mBAAA,CAKM,KALN,EAKM,IALN,EAKM,CAJ4B,IAAA,CAAA,O,kBAAhC,mBAAA,CAA2D,QAA3D,EAA2D;UAAA;AAA9C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,IAAA,IAAA,QAAA,CAAA,IAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAyC,GAA3D,EAA0C,UAA1C,C,qCAII,EAH+B,IAAA,CAAA,O,kBAAnC,mBAAA,CAA+D,QAA/D,EAA+D;UAAA;AAAlD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,OAAA,IAAA,QAAA,CAAA,OAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAA6C,GAA/D,EAA6C,WAA7C,C,qCAGI,EAFiC,IAAA,CAAA,O,kBAArC,mBAAA,CAAkE,QAAlE,EAAkE;UAAA;AAArD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,SAAA,IAAA,QAAA,CAAA,SAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAgD,GAAlE,EAA8C,aAA9C,C,qCAEI,EAD8B,IAAA,CAAA,O,kBAAlC,mBAAA,CAAoE,QAApE,EAAoE;UAAA;AAAvD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,MAAA,IAAA,QAAA,CAAA,MAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAkD,GAApE,EAA4C,iBAA5C,C,qCACI,CALN,CAMI,CATN,CAkBI,EAPJ,mBAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,YAAA,CAAgD,sBAAhD,EAAgD;AAA7B,IAAA,OAAM,EAAE,QAAA,CAAA;AAAqB,GAAhD,EAAgC,IAAhC,EAAgC;AAAA;AAAhC,IAAgC,CAAA,SAAA,CAAhC,CACI,CAFN,CAOI,EAHJ,UAGI,CAnBN,C","sourcesContent":["<template>\n  <div id=\"app\" class=\"app\">\n    <div class=\"header\">\n      <h1>Historias Clinicas</h1>\n\n      <nav>\n        <button v-on:click=\"init\" v-if=\"is_auth\" > Inicio </button>\n        <button v-on:click=\"agregar\" v-if=\"is_auth\" > Agregar </button>\n        <button v-on:click=\"consultar\" v-if=\"is_auth\"> Consultar </button>\n        <button v-on:click=\"logOut\" v-if=\"is_auth\" > Cerrar Sesi贸n </button>\n      </nav>\n    </div>\n    \n    <div class=\"main-component\">\n      <router-view  v-on:log-in=\"logIn\"></router-view>\n    </div>\n\n    <div class=\"footer\">\n      <h2>Contactenos</h2>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport gql from 'graphql-tag'\n\nexport default {\n  name: 'App',\n\n  data: function(){\n      return{ \n        is_auth: false\n      }    \n  },\n\n  created: function(){\n    this.updateAccessToken();\n  },\n\n  methods:{\n\n    updateAccessToken: async function(){\n      if(localStorage.getItem('refresh_token')==null){\n        this.$router.push({name: \"user_auth\"})\n        this.is_auth = false\n        return;\n      }\n\n      await this.$apollo.mutate({\n        mutation: gql`\n          mutation ($refreshTokenRefresh: String!) {\n            refreshToken(refresh: $refreshTokenRefresh) {\n              access\n            }\n          }`\n        , \n        variables: {\n          refreshTokenRefresh: localStorage.getItem('refresh_token')\n        }\n      }).then((result) => {\n        localStorage.setItem('access_token', result.data.refreshToken.access)\n        this.is_auth = true\n      }).catch((error) => {\n        alert(\"Su sesi贸n expir贸, vuelva a iniciar sesi贸n.\")\n        this.$router.push({name: \"user_auth\"})\n        this.is_auth = false\n        localStorage.clear();\n      });\n    },\n\n    logIn: async function(data, username){\n      localStorage.setItem('access_token', data.access)\n      localStorage.setItem('refresh_token', data.refresh)\n      localStorage.setItem('user_id', data.user_id)\n      localStorage.setItem('current_username', username)\n\n      await this.updateAccessToken();\n      if(this.is_auth) this.init();\n    },\n\n    init: function(){\n      this.$router.push({name: \"user\", params:{ username: localStorage.getItem(\"current_username\") }})\n    },\n\n    agregar: function () {\n      this.$router.push({\n        name: \"agregar\",\n        params: { username: localStorage.getItem(\"current_username\") },\n      });\n    },\n\n    consultar: function () {\n      this.$router.push({\n        name: \"consultar\",\n        params: { username: localStorage.getItem(\"current_username\") },\n      });\n      \n    },\n\n    logOut: async function(){\n      localStorage.removeItem('access_token')\n      localStorage.removeItem('refresh_token')\n      localStorage.removeItem('user_id')\n      localStorage.removeItem('current_username')\n\n      await this.updateAccessToken();\n    }   \n  }\n  \n}\n\n</script>\n\n<style>\n  body{\n    margin: 0 0 0 0;\n    \n  }\n\n  .header{\n    position: fixed;\n    left: 0px;\n    top: 0px;\n    width: 100%;\n    height: 78px;\n    margin: 0%;\n    padding: 0;\n\n    background-color: #055ebd;\n    color:#E5E7E9  ;\n\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    box-shadow: 0px 15px 15px #91dff7;\n  }\n\n  .header h1{\n    width: 20%;\n    text-align: center;\n    font-size: 24px;\n    \n  }\n\n  .header nav {\n    height: 100%;\n    width: 40%;\n\n    display: flex;\n    justify-content: space-around;\n    align-items: center;\n\n    font-size: 20px;\n  }\n\n  .header nav button{\n    color: #E5E7E9;\n    background: #055ebd;\n    border: 1px solid #E5E7E9;\n\n    border-radius: 5px;\n    padding: 10px 20px;\n    font-family: Verdana, Geneva, Tahoma, sans-serif;\n    font-weight: normal;\n    font-size: 12px;\n  }\n\n  .header nav button:hover{\n    color: #283747;\n    background: #E5E7E9;\n    border: 1px solid #E5E7E9;\n  }\n\n  .main-component{\n    height: 75vh;\n    margin: 0%;\n    padding: 0%;\n\n    background: #FDFEFE ;\n  }\n \n  .footer{\n    position: fixed;\n    left: 0px;\n    bottom: 0px;\n    width: 100%;\n    height: 75px;\n\n    background-color: #055ebd;\n    color: #E5E7E9;\n    box-shadow: 0px -15px 15px #91dff7; \n  }\n\n  .footer h2{\n    margin: 0px;\n    width: 100%;\n    height: 100%;\n    \n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n</style>\n"],"sourceRoot":""}]}