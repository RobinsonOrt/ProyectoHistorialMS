C,MAAD,EAAY;AAClB,kBAAA,YAAY,CAAC,OAAb,CAAqB,cAArB,EAAqC,MAAM,CAAC,IAAP,CAAY,YAAZ,CAAyB,MAA9D;AACA,kBAAA,KAAI,CAAC,OAAL,GAAe,IAAf;AACD,iBAdK,EAcH,KAdG,CAcG,UAAC,KAAD,EAAW;AAClB,kBAAA,KAAK,CAAC,4CAAD,CAAL;;AACA,kBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAC,oBAAA,IAAI,EAAE;AAAP,mBAAlB;;AACA,kBAAA,KAAI,CAAC,OAAL,GAAe,KAAf;AACD,iBAlBK,CAPW;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAF;;AAAA;AAAA;AAAA;;AAAA;AAAA,OAFX;AA8BN,IAAA,KAAK;AAAA,2EAAE,kBAAe,IAAf,EAAqB,QAArB;AAAA;AAAA;AAAA;AAAA;AACL,gBAAA,YAAY,CAAC,OAAb,CAAqB,cAArB,EAAqC,IAAI,CAAC,MAA1C;AACA,gBAAA,YAAY,CAAC,OAAb,CAAqB,eAArB,EAAsC,IAAI,CAAC,OAA3C;AACA,gBAAA,YAAY,CAAC,OAAb,CAAqB,SAArB,EAAgC,IAAI,CAAC,OAArC;AACA,gBAAA,YAAY,CAAC,OAAb,CAAqB,kBAArB,EAAyC,QAAzC;AAJK;AAAA,uBAMC,KAAK,iBAAL,EAND;;AAAA;AAOL,oBAAG,KAAK,OAAR,EAAiB,KAAK,IAAL;;AAPZ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAF;;AAAA;AAAA;AAAA;;AAAA;AAAA,OA9BC;AAwCN,IAAA,IAAI,EAAE,gBAAU;AACd,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,QAAA,IAAI,EAAE,MAAP;AAAe,QAAA,MAAM,EAAC;AAAE,UAAA,QAAQ,EAAE,YAAY,CAAC,OAAb,CAAqB,kBAArB;AAAZ;AAAtB,OAAlB;AACD,KA1CK;AA4CN,IAAA,MAAM;AAAA,4EAAE;AAAA;AAAA;AAAA;AAAA;AACN,gBAAA,YAAY,CAAC,UAAb,CAAwB,cAAxB;AACA,gBAAA,YAAY,CAAC,UAAb,CAAwB,eAAxB;AACA,gBAAA,YAAY,CAAC,UAAb,CAAwB,SAAxB;AACA,gBAAA,YAAY,CAAC,UAAb,CAAwB,kBAAxB;AAJM;AAAA,uBAMA,KAAK,iBAAL,EANA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAF;;AAAA;AAAA;AAAA;;AAAA;AAAA;AA5CA;AAbK,CAAf","sourcesContent":["<template>\n  <div id=\"app\" class=\"app\">\n    <div class=\"header\">\n      <h1>Historias Clinicas</h1>\n\n      <nav>\n        <button v-on:click=\"init\" v-if=\"is_auth\" > Inicio </button>\n        <button v-on:click=\"logOut\" v-if=\"is_auth\" > Cerrar Sesi贸n </button>\n      </nav>\n    </div>\n    \n    <div class=\"main-component\">\n      <router-view  v-on:log-in=\"logIn\"></router-view>\n    </div>\n\n    <div class=\"footer\">\n      <h2>Contactenos</h2>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport gql from 'graphql-tag'\n\nexport default {\n  name: 'App',\n\n  data: function(){\n      return{ \n        is_auth: false\n      }    \n  },\n\n  created: function(){\n    this.updateAccessToken();\n  },\n\n  methods:{\n\n    updateAccessToken: async function(){\n      if(localStorage.getItem('refresh_token')==null){\n        this.$router.push({name: \"user_auth\"})\n        this.is_auth = false\n        return;\n      }\n\n      await this.$apollo.mutate({\n        mutation: gql`\n          mutation ($refreshTokenRefresh: String!) {\n            refreshToken(refresh: $refreshTokenRefresh) {\n              access\n            }\n          }`\n        , \n        variables: {\n          refreshTokenRefresh: localStorage.getItem('refresh_token')\n        }\n      }).then((result) => {\n        localStorage.setItem('access_token', result.data.refreshToken.access)\n        this.is_auth = true\n      }).catch((error) => {\n        alert(\"Su sesi贸n expir贸, vuelva a iniciar sesi贸n.\")\n        this.$router.push({name: \"user_auth\"})\n        this.is_auth = false\n      });\n    },\n\n    logIn: async function(data, username){\n      localStorage.setItem('access_token', data.access)\n      localStorage.setItem('refresh_token', data.refresh)\n      localStorage.setItem('user_id', data.user_id)\n      localStorage.setItem('current_username', username)\n\n      await this.updateAccessToken();\n      if(this.is_auth) this.init();\n    },\n\n    init: function(){\n      this.$router.push({name: \"user\", params:{ username: localStorage.getItem(\"current_username\") }})\n    },\n\n    logOut: async function(){\n      localStorage.removeItem('access_token')\n      localStorage.removeItem('refresh_token')\n      localStorage.removeItem('user_id')\n      localStorage.removeItem('current_username')\n\n      await this.updateAccessToken();\n    }   \n  }\n  \n}\n\n</script>\n\n<style>\n  body{\n    margin: 0 0 0 0;\n    \n  }\n\n  .header{\n    position: fixed;\n    left: 0px;\n    top: 0px;\n    width: 100%;\n    height: 78px;\n    margin: 0%;\n    padding: 0;\n\n    background-color: #055ebd;\n    color:#E5E7E9  ;\n\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    box-shadow: 0px 15px 15px #91dff7; \n  }\n\n  .header h1{\n    width: 20%;\n    text-align: center;\n    \n  }\n\n  .header nav {\n    height: 100%;\n    width: 40%;\n\n    display: flex;\n    justify-content: space-around;\n    align-items: center;\n\n    font-size: 20px;\n  }\n\n  .header nav button{\n    color: #E5E7E9;\n    background: #055ebd;\n    border: 1px solid #E5E7E9;\n\n    border-radius: 5px;\n    padding: 10px 20px;\n    font-family: Verdana, Geneva, Tahoma, sans-serif;\n    font-weight: bold;\n  }\n\n  .header nav button:hover{\n    color: #283747;\n    background: #E5E7E9;\n    border: 1px solid #E5E7E9;\n  }\n\n  .main-component{\n    height: 75vh;\n    margin: 0%;\n    padding: 0%;\n\n    background: #FDFEFE ;\n  }\n \n  .footer{\n    position: fixed;\n    left: 0px;\n    bottom: 0px;\n    width: 100%;\n    height: 75px;\n\n    background-color: #055ebd;\n    color: #E5E7E9;\n    box-shadow: 0px -15px 15px #91dff7; \n  }\n\n  .footer h2{\n    margin: 0px;\n    width: 100%;\n    height: 100%;\n    \n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n</style>\n"],"sourceRoot":""}]}